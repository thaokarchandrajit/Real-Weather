CREATE EXTERNAL TABLE IF NOT EXISTS location_data_global (
	UID			STRING,
	USAF			STRING,
	WBAN			STRING,
	STATION_NAME		STRING,
	CTRY			STRING,
	FIPS			STRING,
	STATE 			STRING,
	CALL			STRING,
	LAT 			STRING,
	LONG 			STRING,
	ELEV			STRING)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
STORED AS TEXTFILE
LOCATION '/user/testing/location_data';

