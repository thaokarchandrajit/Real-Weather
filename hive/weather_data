CREATE EXTERNAL TABLE IF NOT EXISTS weather_data_global (
	UID				STRING,
	USAF				INT,
	WBAN				INT,
	YEAR				INT,
	MONTH				INT,
	DAY				INT,
	TEMP 				FLOAT,
	COUNT_TEMP			FLOAT,
	DEWP 				FLOAT,
	COUNT_DEWP 			FLOAT,
	SLP				FLOAT,
	COUNT_SLP			FLOAT,
	STP				FLOAT,
	COUNT_STP			FLOAT,
	VISIB				FLOAT,
	COUNT_VISIB			FLOAT,
	WDSP				FLOAT,
	COUNT_WDSP			FLOAT,
	MXSPD				FLOAT,
	GUST				FLOAT,
	MAX				FLOAT,
	FLAG_MAX			STRING,
	MIN				FLOAT,
	FLAG_MIN			STRING,
	PRCP				FLOAT,
	FLAG_PRCP			STRING,
	SNDP				FLOAT,
	FOG				INT,
	RAIN				INT,
	SNOW				INT,
	HAIL				INT,
	THUNDER				INT,
	TORNADO				INT)	
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
STORED AS TEXTFILE
LOCATION '/user/testing/weather_data';


